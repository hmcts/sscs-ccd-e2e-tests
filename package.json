{
  "name": "sscs-ccd-e2e-tests",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "private": true,
  "scripts": {
    "build": "yarn webdriverUpdate && yarn transpile",
    "transpile": "yarn tsc -b",
    "lint": "yarn eslint . && yarn prettier --check .",
    "lint:fix": "yarn prettier --write . && yarn eslint . --fix --quiet",
    "webdriverUpdate": "webdriver-manager update --gecko=false",
    "e2e": "yarn test:fullfunctional",
    "test:local": "yarn build && cross-env NODE_ENV=development protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@nightly-test",
    "test:aat": "yarn build && cross-env NODE_ENV=aat protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@nightly-test",
    "test:fullfunctional": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@nightly-test",
    "test:previewfunctional": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@preview-test",
    "test:crossbrowser": "yarn build && cross-env NODE_ENV=test-crossbrowser protractor ./dist/e2e/features.crossbrowser.conf.js --cucumberOpts.tags='@crossbrowser' --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled",
    "test:prod": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@nightly-test --cucumberOpts.tags=~@issue-decision --cucumberOpts.tags=~@uc --cucumberOpts.tags=~@reinstatement --cucumberOpts.tags=~@bundle",
    "test:a11y": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.a11y.conf.js --cucumberOpts.tags=@a11y",
    "test:nsp": "nsp check"
  },
  "dependencies": {
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@axe-core/webdriverjs": "4.2.1",
    "@faker-js/faker": "^7.6.0",
    "@hmcts/eslint-config": "1.4.0",
    "@hmcts/nodejs-logging": "4.0.4",
    "@types/chai": "4.1.6",
    "@types/config": "^3.3.0",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "axe-core": "4.2.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "codelyzer": "4.5.0",
    "config": "^3.3.8",
    "cross-env": "4.0.0",
    "cucumber": "5.1.0",
    "cucumber-pretty": "1.5.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cucumber": "^2.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "moment": "2.29.4",
    "path": "^0.12.7",
    "prettier": "2.8.1",
    "protractor": "7.0.0",
    "protractor-beautiful-reporter": "1.2.5",
    "protractor-cucumber-framework": "6.1.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.8.1",
    "protractor-retry": "2.0.1",
    "puppeteer": "19.4.1",
    "request-promise": "4.2.5",
    "rxjs": "6.3.3",
    "ts-loader": "5.2.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "resolutions": {}
}
